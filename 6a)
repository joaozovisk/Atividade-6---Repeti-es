programa
{
    funcao inicio()
    {
        inteiro bab1 = 0, aba2 = 1, proximoTermo

        escreva(bab1, ", ", aba2, ", ")

        enquanto (aba2 <= 500)
        {
            proximoTermo = bab1 + aba2
            escreva(proximoTermo, ", ")

            bab1 = aba2
            aba2 = proximoTermo
        }
    }
}
